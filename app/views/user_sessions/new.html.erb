<div class="container">
	<h1>Log In</h1>
	<br />
	<%= form_tag user_sessions_path do %>

		<%= label_tag :username %><br />
		<%= text_field_tag :username, params[:username] %><br />

		<%= label_tag :password %><br />
		<%= password_field_tag :password %><br />

		<%= submit_tag "Log In" %>

	<% end %>	
</div>
