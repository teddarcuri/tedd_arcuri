<div class="container">
	<h1>Update Project</h1>

	<%= form_for :project, url: project_path, method: :patch do |f| %>

	<%= f.label :name %><br />
	<%= f.text_field :name %><br />

	<%= f.text_field :kind %><br />
	
	<%= f.text_area :description %><br />
	<%= f.file_field :image %>

	<%= f.submit("UPDATE project") %>

<% end %>
</div>
